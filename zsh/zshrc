# clear screen
clear

# disable background monitor
setopt no_monitor

# compinit
rm -f ~/.zcompdump &
autoload -Uz compinit
compinit 

# load aliases
source /home/mbednarek360/.config/zsh/aliases.sh

# load plugins
source /home/mbednarek360/.cache/antibody/plugins.sh

# renable background monitor
setopt monitor
